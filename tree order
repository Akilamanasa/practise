class Node:
    def __init__(self,root):
        self.data=root
        self.right=None
        self.left=None 
        
def preorder(root):
        if(root==None):
            return
        print(root.data,end=' ')
        preorder(root.left)
        preorder(root.right)
def inorder(root):
        if (root==None):
            return
        inorder(root.left)
        print(root.data,end=' ')
        inorder(root.right)
def postorder(root):
        if (root==None):
                return
        postorder(root.left)
        postorder(root.right)
        print(root.data,end=' ')
            
root=Node(10)
root.left=Node(5)
root.right=Node(20)
root.left.left=Node(2)
root.left.right=Node(8)
root.right.left=Node(4)
root.right.right=Node(3)
print("Inorder traversal:")
inorder(root)
print("\nPostorder traversal:")
postorder(root)
print("\nPreorder traversal:")
preorder(root)
