class Node:
    def __init__(self,root):
        self.data=root
        self.right=None
        self.left=None
def Top_view(root):
    print()
    q=[]
    while len(q)!=0

root = Node(10)
root.left = Node(5)
root.right = Node(20)
root.left.left = Node(2)
root.left.right = Node(8)
root.right.left = Node(4)
root.right.right = Node(3)
print(" ",Top_view(root))
